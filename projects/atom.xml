---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>Projects</title>
    <link href="http://yimejky.github.io/projects/atom.xml" rel="self"/>
    <link href="http://yimejky.github.io/projects"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://yimejky.github.io/projects</id>
    <author>
        <name>Nikolas Tomastik</name>
        <email>nikolas.tsk@gmail.com</email>
    </author>

    {% for project in site.projects %}
    <entry>
        <title>{{ project.title }}</title>
        <link href="http://yimejky.github.io{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://yimejky.github.io{{ project.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}

</feed>
